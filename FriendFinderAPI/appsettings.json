{
  "ConnectionStrings": {
    "FriendFinderAPIConnection": "Server=(local)\\SQLEXPRESS;Database=FriendFinderAPIDb;Trusted_Connection=True;"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
